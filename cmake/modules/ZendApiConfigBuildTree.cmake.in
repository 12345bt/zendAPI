# - Config file for the Libevent package
# It defines the following variables
#  ZAPI_INCLUDE_DIRS - include directories for FooBar
#  ZAPI_LIBRARIES    - libraries to link against

# Get the path of the current file.
get_filename_component(ZAPI_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

# Set the include directories.
set(ZAPI_INCLUDE_DIRS "@ZAPI_INCLUDE_DIRS@")

# Include the project Targets file, this contains definitions for IMPORTED targets.
include(${ZAPI_CMAKE_DIR}/LibeventTargets.cmake)

# IMPORTED targets from LibeventTargets.cmake
set(ZAPI_LIBRARIES event event_core event_extra)

